{
  "name": "instanssi-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write *.config.ts env.d.ts src/**/*.ts src/**/*.vue",
    "lint-check": "eslint .",
    "format-check": "prettier -c *.config.ts env.d.ts src/**/*.ts src/**/*.vue",
    "generate-api": "openapi-ts -i openapi/instanssi.yaml -c @hey-api/client-axios --name APIClient -o src/api/",
    "fetch-apidoc": "curl -o ./openapi/instanssi.yaml http://localhost:8000/api/v2/openapi/"
  },
  "dependencies": {
    "@fontsource/exo-2": "^5.1.0",
    "@fontsource/fira-mono": "^5.1.0",
    "@fontsource/open-sans": "^5.1.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-brands-svg-icons": "^6.7.1",
    "@fortawesome/free-regular-svg-icons": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@vueuse/core": "^12.0.0",
    "@vueuse/integrations": "^12.0.0",
    "axios": "^1.7.9",
    "lodash-es": "^4.17.21",
    "universal-cookie": "^7.2.2",
    "vee-validate": "^4.14.7",
    "vue": "^3.5.13",
    "vue-i18n": "^10.0.6",
    "vue-router": "^4.5.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.7.5",
    "vuetify-pro-tiptap": "^2.5.4",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.59.1",
    "@hey-api/client-axios": "^0.2.12",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.16.0",
    "@eslint/js": "^9.16.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "typescript": "~5.5.4",
    "vite": "^6.2.5",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-vuetify": "^2.0.4",
    "vue-tsc": "^2.1.10"
  }
