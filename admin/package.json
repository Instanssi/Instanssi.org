{
  "name": "instanssi-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.1",
  "pnpm": {
    "onlyBuiltDependencies": [],
    "onlyBuiltDependenciesFile": "",
    "blockExoticSubdeps": true,
    "minimumReleaseAge": "1440"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "build:production": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint src/ --fix",
    "format": "prettier --write vite.config.ts vitest.config.ts env.d.ts src/**/*.ts src/**/*.vue",
    "lint-check": "eslint src/",
    "format-check": "prettier -c vite.config.ts vitest.config.ts env.d.ts src/**/*.ts src/**/*.vue",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate-api": "openapi-ts -i ./openapi/instanssi.yaml -c @hey-api/client-axios --name APIClient -o src/api/",
    "fetch-apidoc": "curl -o ./openapi/instanssi.yaml http://localhost:8000/api/v2/openapi/"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-vue": "^7.3.0",
    "@fontsource/exo-2": "^5.2.8",
    "@fontsource/fira-mono": "^5.2.7",
    "@fontsource/open-sans": "^5.2.7",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@pdf-lib/fontkit": "^1.1.1",
    "axios": "^1.13.5",
    "chart.js": "^4.5.1",
    "ckeditor5": "^47.4.0",
    "lodash-es": "^4.17.23",
    "pdf-lib": "^1.17.1",
    "temporal-polyfill": "^0.3.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.27",
    "vue-chartjs": "^5.3.3",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "vue-toastification": "2.0.0-rc.5",
    "vuetify": "^3.11.8",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@hey-api/openapi-ts": "^0.85.2",
    "@tsconfig/node24": "^24.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.12",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "^9.33.0",
    "happy-dom": "^20.5.0",
    "prettier": "^3.8.1",
    "sass-embedded": "^1.97.3",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-compression2": "^2.4.0",
    "vite-plugin-vuetify": "^2.1.3",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.4"
  }
}
