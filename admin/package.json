{
  "name": "instanssi-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "format": "prettier --write *.config.ts env.d.ts src/**/*.ts src/**/*.vue",
    "lint-check": "eslint .",
    "format-check": "prettier -c *.config.ts env.d.ts src/**/*.ts src/**/*.vue",
    "generate-api": "openapi-ts -i ./openapi/instanssi.yaml -c @hey-api/client-axios --name APIClient -o src/api/",
    "fetch-apidoc": "curl -o ./openapi/instanssi.yaml http://localhost:8000/api/v2/openapi/"
  },
  "dependencies": {
    "@fontsource/exo-2": "^5.2.8",
    "@fontsource/fira-mono": "^5.2.7",
    "@fontsource/open-sans": "^5.2.7",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "axios": "^1.12.2",
    "lodash-es": "^4.17.23",
    "universal-cookie": "^8.0.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1",
    "vue-toastification": "^2.0.0-rc.5",
    "vuetify": "^3.10.5",
    "vuetify-pro-tiptap": "^2.7.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.85.1",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.18.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "@eslint/js": "^9.37.0",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.9",
    "vite-plugin-compression2": "^2.2.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-tsc": "^3.1.1"
  }
